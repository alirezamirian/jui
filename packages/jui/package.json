    "@react-stately/tree": "~3.8.7",
    "@react-stately/utils": "~3.10.5",
    "@react-stately/virtualizer": "~4.3.1",
    "@react-types/menu": "~3.9.14",
    "@react-types/shared": "~3.27.0",
    "@swc/helpers": "~0.3.17",
    "ramda": "~0.27.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
